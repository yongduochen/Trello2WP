{
  "name": "trello2wp",
  "version": "1.0.0",
  "description": "Export the Trello card as a post to WordPress",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./src -d ./lib -s",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --require babel-register ./test/helper.js ./test/*_test.js",
    "lint": "./node_modules/.bin/eslint .",
    "lint-report": "./node_modules/.bin/eslint . -f ./node_modules/eslint-html-reporter/reporter.js -o eslint-report.html; exit 0",
    "nyc": "cross-env NODE_ENV=test ./node_modules/.bin/nyc --require babel-register node_modules/.bin/_mocha ./test/helper.js ./test/*_test.js",
    "report": "./node_modules/.bin/nyc report --reporter=lcov > coverage.lcov"
  },
  "keywords": [
    "trello",
    "wordpress"
  ],
  "author": "CodeMeow5",
  "license": "MIT",
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "config": "^1.28.1",
    "cross-env": "^5.1.1",
    "ipaddr.js": "^1.5.4",
    "koa": "^2.4.1",
    "koa-better-body": "^3.0.4",
    "koa-router": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.13.3",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "wpapi": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-plugin-mocha": "^4.11.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nock": "^9.1.0",
    "node-fixtures": "0.0.1",
    "nyc": "^11.3.0"
  }
}
